# a comment
key_1=value_1

# an enclosed value
key_2="value_2"

# a value enclosed using single-quotes
key_3='value_3'

# whitespace around value
key_4= value_4

# whitespace around key
 key_5 =value_5

# whitespace around key and value
 key_6 = value_6

# preserved whitespace around value
 key_7 = " value_7 "

# preserved whitespace around value using single-quotes
 key_8 = ' value_8 '

# an empty value
key_9=

# an empty value quoted
key_a= ""

# an empty value quoted using single-quotes
key_b= ''

# a value with an = in it
key_c= a value with an = in it
